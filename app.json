{
  "expo": {
    "name": "Data Collection App",
    "slug": "data-collection-mobile",
    "description": "Data Collection Application. Get approved by Rwanda Space Agency, and Boom! Welcome to storing your data. You can then share it with others or keep it private to you or to your people",
    "scheme": "Data-collection-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconDCA.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundColor": "#064B64"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}